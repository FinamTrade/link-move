<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://github.com/nhl/link-etl/extractor_config.xsd"
	xmlns="http://github.com/nhl/link-etl/extractor_config.xsd">
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<attributes>
		<attribute>
			<type>java.lang.Integer</type>
			<source>age</source>
			<target>age</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>description</source>
			<target>description</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>name</source>
			<target>name</target>
		</attribute>
	</attributes>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT age, description, name FROM utest.etl1
			]]>
		</extractor.jdbc.sqltemplate>
		<extractor.jdbc.sqltemplate.caps>lower
		</extractor.jdbc.sqltemplate.caps>
	</properties>
</config>