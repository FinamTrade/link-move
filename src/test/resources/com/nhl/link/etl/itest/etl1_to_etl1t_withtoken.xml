<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-etl/schema/extractor_config/extractor_config_1.xsd"
	xmlns="http://nhl.github.io/link-etl/schema/extractor_config">
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<attributes>
		<attribute>
			<type>java.lang.Integer</type>
			<source>AGE</source>
			<target>age</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>DESCRIPTION</source>
			<target>description</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>NAME</source>
			<target>name</target>
		</attribute>
	</attributes>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT id, age, description, name FROM utest.etl1 WHERE age > $startToken and age <= $endToken
			]]>
		</extractor.jdbc.sqltemplate>
	</properties>
</config>