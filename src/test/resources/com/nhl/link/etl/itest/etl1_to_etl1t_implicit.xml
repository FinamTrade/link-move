<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-etl/xsd/extractor_config_1.xsd"
	xmlns="http://nhl.github.io/link-etl/xsd/extractor_config_1.xsd">
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT age, description, name FROM utest.etl1
			]]>
		</extractor.jdbc.sqltemplate>
		<!-- This (or #result) is required as Derby metadata returns uppercase columns -->
		<extractor.jdbc.sqltemplate.caps>lower</extractor.jdbc.sqltemplate.caps>
	</properties>
</config>