<?xml version="1.0" encoding="utf-8"?>
<config>
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<attributes>
		<attribute>
			<type>java.lang.Integer</type>
			<source>ID</source>
			<target>db:id</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<source>NAME</source>
			<target>name</target>
		</attribute>
	</attributes>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT id, name, name FROM utest.etl5
			]]>
		</extractor.jdbc.sqltemplate>
	</properties>
</config>