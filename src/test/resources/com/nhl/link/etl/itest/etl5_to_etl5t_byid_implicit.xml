<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-etl/schema/extractor_config/1.0/extractor_config.xsd"
	xmlns="http://nhl.github.io/link-etl/schema/extractor_config">
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT id, name, name FROM utest.etl5
			]]>
		</extractor.jdbc.sqltemplate>
		<!-- This (or #result) is required as Derby metadata returns uppercase columns -->
		<extractor.jdbc.sqltemplate.caps>lower</extractor.jdbc.sqltemplate.caps>
	</properties>
</config>