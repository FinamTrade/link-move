<?xml version="1.0" encoding="utf-8"?>
<!-- ETL within the same DB; connector ID == DataNode name == 'datanode' -->
<config  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://github.com/nhl/link-etl/extractor_config.xsd"
	xmlns="http://github.com/nhl/link-etl/extractor_config.xsd">
	<type>jdbc</type>
	<connectorId>datanode</connectorId>
	<attributes>
		<attribute>
			<type>java.lang.String</type>
			<source>NAME</source>
			<target>name</target>
		</attribute>
	</attributes>
	<properties>
		<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT name FROM utest.etl1t
			]]>
		</extractor.jdbc.sqltemplate>
	</properties>
</config>