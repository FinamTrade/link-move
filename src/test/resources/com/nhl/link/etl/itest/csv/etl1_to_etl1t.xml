<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-etl/schema/extractor_config/1.0/extractor_config.xsd"
	xmlns="http://nhl.github.io/link-etl/schema/extractor_config">
	<type>csv</type>
	<connectorId>/com/nhl/link/etl/itest/csv/etl1.csv</connectorId>
	<attributes>
		<attribute>
			<type>java.lang.Integer</type>
			<target>age</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<target>description</target>
		</attribute>
		<attribute>
			<type>java.lang.String</type>
			<target>name</target>
		</attribute>
	</attributes>
	<properties>
		<extractor.csv.delimiter>;</extractor.csv.delimiter>
		<extractor.csv.readFrom>3</extractor.csv.readFrom>
