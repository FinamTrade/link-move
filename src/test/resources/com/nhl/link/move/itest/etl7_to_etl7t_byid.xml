<?xml version="1.0" encoding="utf-8"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nhl.github.io/link-move/xsd/extractor_config_2.xsd"
	xmlns="http://nhl.github.io/link-move/xsd/extractor_config_2.xsd">
	<type>jdbc</type>
	<connectorId>derbysrc</connectorId>
	<extractor>
		<attributes>
			<attribute>
				<type>java.lang.String</type>
				<source>FULL_NAME</source>
				<target>fullName</target>
			</attribute>
			<attribute>
				<type>java.lang.String</type>
				<source>SEX</source>
				<target>sex</target>
			</attribute>
			<attribute>
				<type>java.lang.Integer</type>
				<source>ID</source>
				<target>db:id</target>
			</attribute>
		</attributes>
		<properties>
			<extractor.jdbc.sqltemplate>
			<![CDATA[SELECT full_name, sex, id FROM utest.etl7
			]]>
			</extractor.jdbc.sqltemplate>
		</properties>
	</extractor>
</config>